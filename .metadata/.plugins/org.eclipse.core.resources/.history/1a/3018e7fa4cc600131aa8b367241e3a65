package calc;

public class calculadora {
 
	public static double soma(int a, int b){
		double value;
		value = a + b;
		return value;
	}
	
	public static double sub(int operand1, int operand2){

		double value;
		value = operand1 - operand2;
		return value;
	}

	public static double div(int operand1, int operand2){

		double value = operand1 / operand2;
		return value;
	}

	public static double mult(int operand1, int operand2){

		double value;
		value = operand1 * operand2;
		return value;
	}
}
