package snakeoop;

public class corpo_add {

	public corpo_add(corposnakecriar[] corpom, int tlb, sNake snnake){

		for(int t = 0; t < corpom.length; t++){
			if(tlb>0){
				if(corpom[t].car == "K"){
					corpom[t].car = "o";
					tlb = tlb-1;
					System.out.print("teste do corpo:"+ corpom[0].car);
				}
			}
		}
		
	}
}
