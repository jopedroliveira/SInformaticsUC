package snakeoop;


public class snakeMovv {
	
	public snakeMovv(sNake snak, int movv){
		System.out.println("movv:" +movv);
		if(movv== 6){
			snak.x = snak.x+1;
			System.out.println("chegou ao a+");
		}
		if(movv == 8){
			snak.y = snak.y+1;
		}
		if(movv == 2){
			snak.y=snak.y-1;
		}
		if(movv == 4){
			snak.x = snak.x+1;
		}
		System.out.println(snak.x);
	}
}

