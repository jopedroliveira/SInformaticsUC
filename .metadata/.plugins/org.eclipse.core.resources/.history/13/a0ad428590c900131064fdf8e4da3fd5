package snakeoop;

public class corpo_add {

	public corpo_add(corposnakecriar[] corpom, int tlb){

		for(int t = 0; t < corpom.length; t++){
			if(tlb == 0){
				break;
			}	
			else{
				if(corpom[t].car == "K"){
					corpom[t].car = "o";
					tlb = tlb-1;
					System.out.println("teste do corpo:"+ corpom[0].car + "tl: "+tlb);
				}
				else{
					System.out.println("tlb a zero");
				}
			}
		}

	}
}
