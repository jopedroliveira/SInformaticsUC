package snakeoop;

import java.util.Random;

public class inimigoMover {

	public inimigoMover(criarObjeto[] inimigu, String[][] mapax){
		Random rand = new Random(); 
		for(int i=0; i<inimigu.length;i++){
			inimigu[i].x = inimigu[i].x + (rand.nextInt(2)-1);
			inimigu[i].y = inimigu[i].y + (rand.nextInt(2)-1);
		}

	}

}
