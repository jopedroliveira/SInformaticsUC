package ex5;

import java.util.Random;

public class Retangulo {
	//atributos
	public int altura;
	public int largura;
	public double posicaox;
	public double posicaoy;

	public Retangulo(){
		Random rand = new Random();
		this.altura = rand.nextInt(10) + 1;
		this.largura = rand.nextInt(20)+1;
	}


}
