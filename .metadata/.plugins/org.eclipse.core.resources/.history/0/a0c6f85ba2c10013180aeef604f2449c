package calc;

public class calculadora {
 
	public static float soma(int operand1, int operand2){
		float value;
		value = operand1 + operand2;
		return value;
	}
	
	public static float sub(int operand1, int operand2){

		float value;
		value = operand1 - operand2;
		return value;
	}

	public static float div(int operand1, int operand2){

		float value;
		value = operand1 / operand2;
		return value;
	}

	public static float product(int operand1, int operand2){

		float value;
		value = operand1 * operand2;
		return value;
	}
}
