package snakeoop;


public class snakeMovv {
	
	public snakeMovv(sNake snak, int movv){
		System.out.println("movv:" +movv);
		if(movv== 8){
			snak.y = snak.y+1;
		}
		else if(movv == 6){
			snak.x = snak.x+1;
		}
		else if(movv == 5){
			snak.y=snak.y-1;
		}
		else if(movv == 4){
			snak.x = snak.x-1;
		}
	}
}

