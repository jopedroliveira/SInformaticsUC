package exercicio3.java;

public class Retangulo {
	
	
	public static void main(String[] args){
		retan = new construtor();
		System.out.println("altura: "+ consturtor.altura + )
	}
	
	public altura;
	public largura;
	public posicao;
	
	public construtor(){
		int x,y;
		this.altura = 10;
		this.largura = 20;
		this.posicao = posi();
