package snakeoop;

public class corpo_add {

	public corpo_add(corposnakecriar[] corpom, int tlb){

		for(int t = 0; t < corpom.length; t++){
				if(corpom[t].car == "K"){
					corpom[t].car = "o";
					System.out.println("teste do corpo:"+ corpom[0].car);
					tlb = tlb-1;
					if(tlb == 0){
						break;
					}
				}
			}
		}
		
	}
}
