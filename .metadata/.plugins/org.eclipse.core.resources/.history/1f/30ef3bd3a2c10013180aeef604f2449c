package test;

import calc.calculadora;

public class exercicio51 {
	public static void main(String[] args){

		float a = 9;
		float b = 2;
		System.out.println("soma: " + calculadora.soma(a,b));
		System.out.println("subtraccao: " +  calculadora.sub(a,b));
		System.out.println("multiplicacao: " + (float) calculadora.mult(a,b));
		System.out.println("divisao: "+(float) calculadora.div(a,b));
	}
}