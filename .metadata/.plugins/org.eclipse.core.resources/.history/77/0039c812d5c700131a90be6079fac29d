package snakeoop;

import java.util.Scanner;

public class printMapa {

	public printMapa(String[][] mapamapaq){
		for(int ii = 0; ii< mapamapaq.length; ii++){ 
			for(int jj = 0; jj<mapamapaq[0].length;jj++){
				System.out.print(mapamapaq[ii][jj]);
			}
			System.out.print("\n");
		}
	}
	
	
	public static sNake snakeMove(sNake snake){
		String up = "w";
		String left = "a";
		String righ = "d";
		String down = "s";
		//int TTL = 3;
		//int TTLbonus = 3;
		
		Scanner com = new Scanner(System.in);
		String mov = com.nextLine();

		if(mov == up){
			snake.y = snake.y+1;
		}
		else if(mov == down){
			snake.y=snake.y-1;
		}
		else if(mov == left){
			snake.x = snake.x-1;
		}
		else if(mov == righ){
			snake.x = snake.x+1;
		}
		return snake;
	}
}


