package test;

import calc.calculadora;

public class exercicio51 {
	public static void main(String[] args){

		int a = 9;
		int b = 2;
		int res = (int) calculadora.soma(a,b);
		System.out.println("soma: " + res);
		System.out.println("subtraccao: " + (int) calculadora.sub(a,b));
		System.out.println("multipllicacao: " + (int)calculadora.mult(a,b));
	}
}