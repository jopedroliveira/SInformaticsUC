package snakeoop;


public class snakeMove {
	private int x,y;
	public snakeMove(sNake snak, String mov){
		
		System.out.println(mov);
		String up = "w";
		String left = "a";
		String righ = "d";
		String down = "s";
		//int TTL = 3;
		//int TTLbonus = 3;
		
		
		if(mov == "w"){
			this.y = snak.y+1;
		}
		else if(mov == "s"){
			this.y=snak.y-1;
		}
		else if(mov == "a"){
			this.x = snak.x-1;
		}
		else if(mov == "d"){
			this.x = snak.x+1;
		}
		System.out.println("x velho:" + this.x);
	}
}

