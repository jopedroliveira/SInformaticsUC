package test;

import calc.calculadora;

public class exercicio51 {
	public static void main(String[] args){

		int a = 9;
		int b = 2;
		System.out.println("soma: " + (float) calculadora.soma(a,b));
		System.out.println("subtraccao: " + (float) calculadora.sub(a,b));
		System.out.println("multiplicacao: " + (float) calculadora.mult(a,b));
		System.out.println("divisao: "+(double) calculadora.div(a,b));
	}
}